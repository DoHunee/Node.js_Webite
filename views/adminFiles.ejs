<%-include('./base/header.ejs')%> 

<!DOCTYPE html>
<html>
<head>
    <title>관리자 파일 목록</title>
</head>
<body>
    <h1>업로드된 파일 목록</h1>
    <ul>
        <% files.forEach(file => { %>
            <li>
                <a href="/uploads/<%= file.filename %>" target="_blank"><%= file.filename %></a>
                <!-- 파일을 직접 보기 위한 링크. 'uploads' 폴더가 정적 디렉토리로 설정되어 있어야 함 -->
            </li>
        <% }) %>
    </ul>
</body>
</html>


<%-include('./base/footer.ejs')%>     